<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
        	<solid
        		android:color="#73ff66" />
            <stroke
                android:width="1dp"
                android:color="#33cb33" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#73ff66"
                android:endColor="#5ac94f"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#33cb33" />
            <corners
                android:radius="4dp" />
        </shape>
    </item>
</selector>
